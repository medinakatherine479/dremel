<script type="text/javascript">

    function toggleView() {
        if (isUserLoggedIn()) {

            let loginButtons = document.querySelectorAll(".M-ProfileAccess__userLoginButton");
            for (let i = 0; i < loginButtons.length; i++) {
                loginButtons[i].style.display = "none";
            }

            let loggedInMenus = document.querySelectorAll(".M-ProfileAccess__userLoggedIn");
            for (let i = 0; i < loggedInMenus.length; i++) {
                loggedInMenus[i].style.display = "block";
            }

            let userIdTextFields = document.querySelectorAll(".M-ProfileAccess-info__userId");
            for (let i = 0; i < userIdTextFields.length; i++) {
                userIdTextFields[i].innerText = "User ID: " + getUserId();
            }
        }
    }
</script></header>



